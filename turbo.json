{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [".env", "./packages/prisma/.env"],
  "globalEnv": ["ANALYZE"],
  "pipeline": {
    "@lib/prisma#build": {
      "cache": false,
      "inputs": ["schema.prisma", "prisma/schema.prisma"],
      "env": ["DATABASE_URL"],
      "outputs": [
        "../../node_modules/@prisma/client/**",
        "../../node_modules/.prisma/client/**"
      ]
    },
    "@lib/prisma#deploy": {
      "dependsOn": ["build"],
      "env": ["DATABASE_URL"],
      "inputs": ["prisma/migrations/**/*.sql"],
      "outputs": []
    },
    "@lib/prisma#push": {
      "dependsOn": ["build"],
      "cache": false,
      "outputs": []
    },
    "pocket-contract#build": {
      "inputs": ["**/*.sol"],
      "outputs": ["artifacts/**", "typechain-types/**", "cache/**", "abi/**"]
    },
    "fork": {
      "cache": false
    },
    "clean": {
      "dependsOn": ["^clean"],
      "cache": false
    },
    "build": {
      "dependsOn": ["@lib/prisma#deploy", "^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "dependsOn": ["@lib/prisma#push"],
      "cache": false
    },
    "reset": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    }
  }
}
