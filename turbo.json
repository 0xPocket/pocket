{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [".env", "./packages/prisma/.env"],
  "pipeline": {
    "@lib/prisma#build": {
      "inputs": ["./prisma/schema.prisma", ".env"],
      "dependsOn": ["$DATABASE_URL"],
      "outputs": ["./../../node_modules/@prisma/client"]
    },
    "@lib/prisma#deploy": {
      "dependsOn": ["build"],
      "inputs": ["./prisma/migrations/**/*.sql"],
      "cache": false,
      "outputs": []
    },
    "@lib/prisma#push": {
      "dependsOn": ["build"],
      "inputs": ["./prisma/migrations/**/*.sql"],
      "cache": false,
      "outputs": []
    },
    "pocket-contract#build": {
      "inputs": ["**/*.sol"],
      "outputs": ["./artifacts/**/*", "./typechain-types/**/*", "./cache/**/*"]
    },
    "pocket-contract#deploy": {
      "dependsOn": ["build"],
      "cache": false
    },
    "fork": {
      "cache": false
    },
    "clean": {
      "dependsOn": ["^clean"],
      "cache": false
    },
    "web#build": {
      "dependsOn": ["@lib/prisma#deploy"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "dependsOn": [
        "//#env-check",
        "@lib/prisma#push",
        "pocket-contract#deploy"
      ],
      "cache": false
    },
    "reset": {
      "cache": false
    },
    "dev-testnet": {
      "dependsOn": ["//#env-check", "@lib/prisma#push"],
      "cache": false
    },
    "start": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "test:e2e": {
      "cache": false
    },
    "//#env-check": {
      "inputs": ["./.env.example", "./.env"],
      "outputs": ["./.env"]
    }
  }
}
