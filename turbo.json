{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [".env", "./packages/prisma/.env"],
  "globalEnv": ["ANALYZE"],
  "pipeline": {
    "db:generate": {
      "inputs": ["prisma/schema.prisma"],
      "outputs": [
        "./../../node_modules/@prisma/client/**",
        "./../../node_modules/.prisma/client/**"
      ]
    },
    "db:push": {
      "dependsOn": ["db:generate"],
      "cache": false
    },
    "db:deploy": {
      "dependsOn": ["db:generate"],
      "env": ["DATABASE_URL"],
      "inputs": ["prisma/migrations/**/*.sql"],
      "outputs": []
    },
    "db:seed": {
      "cache": false
    },
    "pocket-contract#build": {
      "inputs": ["**/*.sol"],
      "outputs": ["artifacts/**", "typechain-types/**", "cache/**", "abi/**"]
    },
    "fork": {
      "cache": false
    },
    "clean": {
      "dependsOn": ["^clean"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^db:generate", "^db:deploy", "^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "dependsOn": ["^db:generate", "^db:push", "^db:seed"],
      "cache": false
    },
    "reset": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    }
  }
}
