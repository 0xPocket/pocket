{
  "globalDependencies": [".env", "packages/prisma/.env"],
  "pipeline": {
    "@lib/prisma#build": {
      "dependsOn": ["$DATABASE_URL"]
    },
    "@lib/prisma#db:deploy": {
      "dependsOn": ["$DATABASE_URL"]
    },
    "@lib/prisma#db:reset": {
      "cache": false
    },
    "@lib/prisma#db:down": {
      "cache": false
    },
    "@lib/prisma#db:seed": {
      "cache": false,
      "dependsOn": ["@lib/prisma#db:deploy"]
    },
    "@lib/prisma#dev": {
      "cache": false
    },
    "blockchain-side-pocket#dev": {
      "dependsOn": ["@lib/contract#healthcheck"],
      "cache": false
    },
    "@lib/contract#healthcheck": {
      "cache": false
    },
    "api#dev": {
      "cache": false,
      "dependsOn": ["blockchain-side-pocket#dev", "@lib/prisma#dev"]
    },
    "clean": {
      "dependsOn": ["@lib/prisma#db:down"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "test:e2e": {
      "cache": false,
      "dependsOn": ["@lib/prisma#db:reset"]
    }
  }
}
