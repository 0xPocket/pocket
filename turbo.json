{
  "globalDependencies": [
    ".env",
    "packages/prisma/.env",
    "packages/pocket-contract/.env"
  ],
  "pipeline": {
    "@lib/prisma#build": {
      "dependsOn": ["$DATABASE_URL"]
    },
    "@lib/prisma#db:deploy": {
      "dependsOn": ["$DATABASE_URL"]
    },
    "@lib/prisma#db:reset": {
      "cache": false
    },
    "@lib/prisma#db:down": {
      "cache": false
    },
    "@lib/prisma#db:seed": {
      "cache": false,
      "dependsOn": ["@lib/prisma#db:deploy"]
    },
    "@lib/prisma#dev": {
      "cache": false
    },
    "pocket-contract#dev": {
      "cache": false
    },
    "pocket-contract#build:types": {
      "cache": false
    },
    "pocket-contract#healthcheck": {
      "cache": false
    },
    "pocket-contract#deploy": {
      "dependsOn": ["pocket-contract#healthcheck"],
      "cache": false
    },
    "pocket-contract#seed": {
      "dependsOn": ["pocket-contract#deploy"],
      "cache": false
    },
    "web-parent#dev": {
      "cache": false,
      "dependsOn": ["pocket-contract#deploy"]
    },
    "web-child#dev": {
      "cache": false,
      "dependsOn": ["pocket-contract#deploy"]
    },
    "api#dev": {
      "cache": false,
      "dependsOn": [
        "@lib/prisma#dev",
        "pocket-contract#deploy",
        "pocket-contract#seed"
      ]
    },
    "api#dx": {
      "cache": false,
      "dependsOn": ["@lib/prisma#dev"]
    },
    "clean": {
      "dependsOn": ["@lib/prisma#db:down"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "cache": false
    },
    "dx": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "test:e2e": {
      "cache": false,
      "dependsOn": ["@lib/prisma#db:reset"]
    }
  }
}
