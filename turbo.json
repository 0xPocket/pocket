{
  "globalDependencies": [
    ".env",
    "packages/prisma/.env",
    "packages/pocket-contract/.env"
  ],
  "pipeline": {
    "@lib/prisma#build": {
      "dependsOn": ["$DATABASE_URL"]
    },
    "@lib/prisma#db:deploy": {
      "dependsOn": ["$DATABASE_URL"]
    },
    "@lib/prisma#db:seed": {
      "cache": false,
      "dependsOn": ["deploy"]
    },
    "pocket-contract#healthcheck": {
      "cache": false
    },
    "pocket-contract#build": {
      "outputs": ["artifacts/**/*", "typechain-types/**/*"],
      "inputs": ["contracts/**/*.sol"]
    },
    "pocket-contract#deploy": {
      "dependsOn": ["pocket-contract#build", "healthcheck"],
      "cache": false
    },
    "pocket-contract#seed": {
      "dependsOn": ["deploy"],
      "cache": false
    },
    "web-parent#dev": {
      "cache": false,
      "dependsOn": ["pocket-contract#deploy"]
    },
    "web-child#dev": {
      "cache": false,
      "dependsOn": ["pocket-contract#deploy"]
    },
    "api#dev": {
      "cache": false,
      "dependsOn": ["pocket-contract#seed"]
    },
    "api#dx": {
      "cache": false,
      "dependsOn": ["@lib/prisma#dev"]
    },
    "clean": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".next/**",
        "artifacts/**/*",
        "typechain-types/**/*"
      ]
    },
    "dev": {
      "cache": false
    },
    "dx": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "test:e2e": {
      "cache": false,
      "dependsOn": ["@lib/prisma#db:reset"]
    }
  }
}
