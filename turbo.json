{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [".env", "./packages/prisma/.env", "$ANALYZE"],
  "pipeline": {
    "@lib/prisma#build": {
      "inputs": ["./schema.prisma", "./prisma/schema.prisma"],
      "dependsOn": ["$DATABASE_URL"],
      "outputs": [
        "../../node_modules/@prisma/client/**",
        "../../node_modules/.prisma/client/**"
      ]
    },
    "@lib/prisma#deploy": {
      "dependsOn": ["build"],
      "inputs": ["./prisma/migrations/**/*.sql"],
      "cache": false,
      "outputs": []
    },
    "@lib/prisma#push": {
      "dependsOn": ["build"],
      "inputs": ["./prisma/migrations/**/*.sql"],
      "cache": false,
      "outputs": []
    },
    "pocket-contract#build": {
      "inputs": ["**/*.sol"],
      "outputs": ["./artifacts/**/*", "./typechain-types/**/*", "./cache/**/*"]
    },
    "pocket-contract#deploy": {
      "dependsOn": ["build"],
      "cache": false
    },
    "fork": {
      "cache": false
    },
    "clean": {
      "dependsOn": ["^clean"],
      "cache": false
    },
    "build": {
      "dependsOn": ["@lib/prisma#deploy", "^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "dependsOn": ["@lib/prisma#push"],
      "cache": false
    },
    "reset": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    }
  }
}
