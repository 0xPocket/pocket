{
  "globalDependencies": [".env", "packages/prisma/.env"],
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "@lib/prisma#generate": {
      "inputs": ["./schema.prisma"]
    },
    "@lib/prisma#deploy": {
      "dependsOn": ["generate"],
      "inputs": ["./migrations/**/*.sql"],
      "cache": false,
      "outputs": []
    },
    "@lib/prisma#seed": {
      "dependsOn": ["deploy"],
      "cache": false
    },
    "pocket-contract#build": {
      "inputs": ["./contracts/**/*.sol"],
      "outputs": ["./artifacts/**/*", "./typechain-types/**/*", "./cache/**/*"]
    },
    "pocket-contract#deploy": {
      "dependsOn": ["build"],
      "cache": false
    },
    "pocket-contract#seed": {
      "dependsOn": ["deploy"],
      "cache": false
    },
    "hardhat:start": {
      "cache": false
    },
    "clean": {
      "dependsOn": ["^clean"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "dependsOn": ["//#env-check", "@lib/prisma#seed", "pocket-contract#seed"],
      "outputs": [],
      "cache": false
    },
    "start": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "test:e2e": {
      "cache": false
    },
    "//#env-check": {
      "inputs": ["./.env.example", "./.env"],
      "outputs": ["./.env"]
    }
  }
}
