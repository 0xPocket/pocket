{
  "globalDependencies": [".env", "packages/prisma/.env"],
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "@lib/prisma#generate": {
      "outputs": [
        "../../node_modules/@prisma/client/**",
        "../../node_modules/@prisma/admin-client/**"
      ],
      "inputs": ["./schema.prisma", "./prisma/schema.prisma"]
    },
    "@lib/prisma#db:deploy": {
      "dependsOn": ["generate"],
      "inputs": ["./migrations/**/*.sql", "./prisma/migrations/**/*.sql"],
      "cache": false,
      "outputs": []
    },
    "@lib/prisma#db:seed": {
      "cache": false,
      "dependsOn": ["@lib/prisma#db:deploy"]
    },
    "pocket-contract#build": {
      "outputs": ["./artifacts/**/*", "./typechain-types/**/*", "./cache/**/*"]
    },
    "pocket-contract#deploy": {
      "dependsOn": ["pocket-contract#build"],
      "cache": false
    },
    "pocket-contract#seed": {
      "dependsOn": ["pocket-contract#deploy"],
      "cache": false
    },
    "hardhat:start": {
      "cache": false
    },
    "hardhat:deploy": {
      "cache": false
    },
    "clean": {
      "dependsOn": ["^clean"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "dependsOn": [
        "//#env-check",
        "@lib/prisma#db:deploy",
        "pocket-contract#build",
        "pocket-contract#seed"
      ],
      "outputs": [],
      "cache": false
    },
    "start": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "test:e2e": {
      "cache": false
    },
    "//#env-check": {
      "inputs": ["./.env.example", "./.env"],
      "outputs": ["./.env"]
    }
  }
}
